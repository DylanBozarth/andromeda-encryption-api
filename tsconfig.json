{
    "compilerOptions": {
      "target": "es3",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "noImplicitAny": false,
      "plugins": [{ "name": "typescript-plugin-css-modules" }]
    }
  }